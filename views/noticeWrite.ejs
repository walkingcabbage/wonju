<div class="notice-write-wrap">
    <div class="notice-holder">
        <section class="notice-write-top">
            <div class="top-box">
                <h2 class="title">공지사항 작성</h2>
            </div>
        </section>
        <section class="notice-write-middle">
            <form class="notice-write-form" method="post" action="/writeNotice">
                <div class="tit-box">
                    <p>제목</p>
                    <input type="text" name="title">
                </div>
                <div class="content-box">
                    <p>내용</p>
                    <textarea name="content" id="" ></textarea>
                </div>
                <button onclick="location.href='/noticePage'" class="btns">돌아가기</button>
                <input class="btns" type="submit" value="저장">
            </form>
        </section>
    </div>
</div>